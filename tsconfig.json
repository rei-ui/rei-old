{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["es2017", "esnext.asynciterable", "dom"],
    "declaration": true,
    "sourceMap": true,
    "inlineSources": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "outDir": "./lib",
    "rootDir": "./src/lib"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": []
}
